{
  "name": "splitiv",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.15.1",
    "pnpm": "9.6.0"
  },
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "check:prettier": "prettier \"**/*.{css,json,md,js,jsx,ts,tsx}\"",
    "check:lint": "next lint",
    "check:types": "tsc",
    "fix:prettier": "prettier \"**/*.{css,json,md,js,jsx,ts,tsx}\" --write",
    "fix:lint": "next lint --fix",
    "dev": "docker compose up -d && prisma generate && concurrently --kill-others \"docker compose logs -f\" \"prisma studio -b none\" \"next dev\"",
    "test": "vitest",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@prisma/client": "^5.13.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@serwist/next": "^9.0.2",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.35.1",
    "@tanstack/react-query-devtools": "^5.35.1",
    "@trpc/client": "next",
    "@trpc/next": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "arctic": "^1.8.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "decimal.js": "^10.4.3",
    "jotai": "^2.8.0",
    "lucia": "^3.2.0",
    "lucide-react": "^0.378.0",
    "next": "14.2.3",
    "oslo": "^1.2.0",
    "rc-input-number": "^9.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.51.4",
    "react-intersection-observer": "^9.10.2",
    "sonner": "^1.4.41",
    "superjson": "2.2.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.1",
    "web-push": "^3.6.7",
    "zod": "^3.23.7"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.32.1",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.12.10",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/web-push": "^3.6.3",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "happy-dom": "^14.10.1",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prisma": "^5.13.0",
    "serwist": "^9.0.2",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged --concurrent false && pnpm tsc"
  },
  "lint-staged": {
    "*.{css,json,md,js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "*.{,js,jsx,ts,tsx}": [
      "eslint --fix",
      "vitest related --run"
    ]
  }
}
